class A 
{
    public static void main(String[]args)
    {
        int n=5;
        int num=1;
        for(int i=1;i<=n;i++)
        {
                if(i%2!=0)
                { 
                    num=i*5-4;
                    for(int k=1;k<=n;k++)
                    {
                System.out.print(num+"\t");
                num++;
                    }
                }
                else
                {
                    num=i*5;
                    for(int j=1;j<=n;j++)
                    {
                    System.out.print(num+"\t");
                    num--;
                    }
                }
            System.out.println();
        }
    }
}
